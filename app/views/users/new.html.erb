<h1>New User Form</h1>
<%= form_for @user do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <br>
  <%= f.label :age %>
  <%= f.text_field :age%>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit 'New User'%>
<%end%>
