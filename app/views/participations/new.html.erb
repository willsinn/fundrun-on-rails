
<%= form_for @participation do |f| %>
  <%= f.hidden_field :user_id, value: session[:user_id]%>
  <%# user.id needs to be an user object id %>

  <%= f.hidden_field :marathon_id, value: @marathon.id %>
  <%#  marathon.id needs to be the current marathon you're trying to associate %>
  <%= f.submit "Follow" %>
<% end  %>
